{
  "author": "DaanV2",
  "description": "Generates links using markdown tags",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "markdown-tags",
  "version": "1.0.0",
  "keywords": [
    "github",
    "action",
    "markdown",
    "linking",
    "tags"
  ],
  "scripts": {
    "compile": "tsc -b && ncc build ./out/src/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "precompile": "tsc -b --clean",
    "prepublishOnly": "npm test && npm run compile",
    "tags": "ts-node src/index.ts",
    "pretest": "npm run compile",
    "test": "jest",
    "preversion": "npm run compile && git add -A dist",
    "version": "git add -A src && git add -A dist",
    "postversion": "git push && git push --tags",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "fast-glob": "^3.3.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@vercel/ncc": "^0.36.0",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
